{
  "name": "@easykv/easykv",
  "version": "2.0.2",
  "exports": "./mod.ts",
  "license": "MIT",
  "publish": {
    "include": ["LICENSE", "mod.ts", "README.md", "deno.json", "lib/"],
    "exclude": [
      "main_test.ts",
      "**/*.test.ts",
      ".vscode/",
      ".idea/",
      ".*",
      "Documents/",
      "*.lock",
      "*.log"
    ]
  },
  "tasks": {
    "dev": "deno run --watch --allow-all --unstable-kv mod.ts",
    "test": "deno test --allow-all --unstable-kv main_test.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
